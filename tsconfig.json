{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "rootDir": "./src",
      "outDir": "./dist",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "moduleResolution": "node",
      "baseUrl": "./"
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
  }
  